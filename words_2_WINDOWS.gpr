project Words_2_WINDOWS is

   for Languages use ("Ada");
   for Source_Dirs use ("main", "main\windows","dictionary");
   for Main use ("words.adb");
   for Object_Dir use "obj";
   for Exec_Dir use "obj";

   package Compiler is
      for Default_Switches ("ada") use ("-gnato", "-fstack-check");
   end Compiler;

   package Builder is
      for Switches ("ada") use ("-j3","-O2");
   end Builder;

end Words_2_WINDOWS;

