   for Source_Dirs use ("main", "main\windows","dictionary","main\nonunicode");
   for Main use ("words.adb");
   for Object_Dir use "obj";
   for Exec_Dir use "obj";

   package Builder is
      for Switches ("ada") use ("-j4","-O2");
   end Builder;

end Words_2_WINDOWS;

